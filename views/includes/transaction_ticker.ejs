<form class="transaction-form" name="tickerFinder" onsubmit="searchTicker(this); return false;">
    <div class="form-control">
        <label for="ticker-symbol">Ticker</label>
        <input type="text" name="ticker" id="ticker-symbol" value=<%=ticker%>>
        <label for="price">Price</label>
        <input disabled type="number" name="price" id="ticker-price" value=<%=tickerPrice%> >
        <button type="submit" name="tickerFinder" id="tickerFinder">Check Price</button>
   </div>
</form>